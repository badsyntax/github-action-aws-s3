{
  "name": "github-action-aws-s3",
  "version": "0.0.0",
  "private": true,
  "description": "A GitHub Action to create/update your CloudFormation stack",
  "main": "lib/main.js",
  "type": "module",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "build:ide": "npm run build -- --sourceMap",
    "format": "prettier --write '**/*.{ts,json,svg,md,yml}'",
    "format-check": "prettier --check '**/*.{ts,json,svg,md,yml}'",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build lib/main.js --source-map --license licenses.txt",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest"
  },
  "engines": {
    "node": "^16",
    "npm": "^8"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/badsyntax/github-action-aws-s3.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup",
    "aws",
    "s3",
    "multipart",
    "md5",
    "etag"
  ],
  "author": "Richard Willis <willis.rh@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.6.0",
    "@actions/github": "^5.0.0",
    "@aws-sdk/client-s3": "^3.53.1",
    "@aws-sdk/lib-storage": "^3.229.0",
    "glob": "^7.2.0",
    "mime-types": "^2.1.34",
    "minimatch": "^5.0.1",
    "s3-etag": "^1.0.3"
  },
  "devDependencies": {
    "@octokit/webhooks-definitions": "^3.67.3",
    "@types/glob": "^7.2.0",
    "@types/mime-types": "^2.1.1",
    "@types/minimatch": "^3.0.5",
    "@types/node": "^17.0.0",
    "@typescript-eslint/parser": "^5.23.0",
    "@vercel/ncc": "^0.33.3",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-github": "^4.3.5",
    "eslint-plugin-jest": "^26.1.1",
    "jest": "^27.5.1",
    "prettier": "2.5.1",
    "ts-jest": "^27.1.3",
    "typescript": "^4.6.4"
  }
}
